{
  "name": "dmart",
  "version": "1.0.0",
  "private": true,
  "description": "DMart seeks to be a one-stop shopping destination for the entire family, meeting all their daily household needs.",
  "homepage": "http://www.dmartindia.com/",
  "scripts": {
    "start": "gulp serve --open",
    "postinstall": "bower i"
  },
  "authors": [
    "Brajeshwar, http://brajeshwar.me",
    "Razorfish, http://razorfish.com"
  ],
  "dependencies": {
    "wiredep": "^2.2.2"
  },
  "devDependencies": {
    "autoprefixer-core": "~5.2.0",
    "babel-core": "~5.6.15",
    "babel-eslint": "~4.0.0",
    "babelify": "~6.0.2",
    "browser-sync": "~2.7.12",
    "browserify": "~10.0.0",
    "del": "~1.2.0",
    "envify": "~3.4.0",
    "foldero": "~0.1.1",
    "glob": "~5.0.10",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.2.1",
    "gulp-data": "~1.2.0",
    "gulp-eslint": "~0.14.0",
    "gulp-htmlmin": "~1.1.3",
    "gulp-iconfont": "^4.0.1",
    "gulp-if": "~1.2.5",
    "gulp-imagemin": "~2.2.1",
    "gulp-load-plugins": "~0.10.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-newer": "~0.5.1",
    "gulp-nunjucks-html": "~1.2.2",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "^6.0.0",
    "gulp-print": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "~2.0.1",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-uglify": "~1.2.0",
    "gulp-useref": "^1.3.0",
    "gulp-util": "~3.0.5",
    "imagemin-pngquant": "~4.0.0",
    "imagemin-svgo": "~4.0.0",
    "karma": "~0.12.21",
    "karma-browserify": "~4.2.1",
    "karma-jasmine": "~0.2.0",
    "karma-phantomjs-launcher": "~0.1.2",
    "lodash": "^3.10.1",
    "minimist": "~1.1.1",
    "nunjucks": "~1.3.4",
    "rimraf": "~2.2.0",
    "run-sequence": "^1.1.2",
    "through2": "^2.0.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.3.0 ",
    "wrench": "~1.5.8"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "/",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "icons": "_icons",
      "styles": "_styles",
      "fonts": "fonts",
      "bower": "_bower_components",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
